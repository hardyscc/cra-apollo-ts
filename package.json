{
  "name": "cra-apollo-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.1.4",
    "graphql": "^0.13.2",
    "react": "^16.3.2",
    "react-apollo": "^2.1.3",
    "react-dom": "^16.3.2",
    "react-scripts-ts": "2.15.1"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "schema": "apollo-codegen introspect-schema https://portfolios.now.sh --output ./src/schema.json",
    "types": "apollo-codegen generate ./src/**/*.tsx --addTypename --schema ./src/schema.json --target typescript --output ./src/__generated__/types.ts"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.2",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "apollo-codegen": "^0.19.1",
    "react-testing-library": "^2.4.0",
    "typescript": "^2.8.3"
  },
  "jest": {
    "collectCoverageFrom": ["src/components/**/*.{ts,tsx}"]
  }  
}
